{
  "files.associations": {
    ".env-pro": "dotenv",
    ".env-stg": "dotenv"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vsicons.presets.nestjs": true,
  "vsicons.associations.folders": [
    {
      "icon": "bloc",
      "extensions": ["modules"]
    },
    {
      "icon": "sso",
      "extensions": ["guards"]
    }
  ],
  "vsicons.associations.files": [
    {
      "icon": "tsconfig",
      "extensions": ["tsconfig.build.json"],
      "filename": true
    }
  ]
}
